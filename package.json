{
  "name": "fillory",
  "version": "0.0.0",
  "description": "React UI component library",
  "main": "index.js",
  "private": true,
  "scripts": {
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "storybook": "start-storybook -p 6006 --ci",
    "storybook:build": "build-storybook",
    "prepublishOnly": "npm run build",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development rollup -c && move-cli lib/dev.css lib/styles.css",
    "build:prod": "cross-env NODE_ENV=production rollup -c && move-cli lib/prod.css lib/styles.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ui",
    "react",
    "react-component"
  ],
  "author": {
    "name": "Vladislav Danilovich",
    "email": "cevladka@yandex.ru"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.5",
    "react-dom": "^16.8.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "move-cli": "^1.2.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react": "^16.8.5",
    "rollup": "^1.7.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "latest"
  }
}
